{
  "name": "eslint-config-rajzik",
  "description": "Reusable ESLint config for personal projects",
  "version": "1.0.5",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@rajzik/configs-shared": "workspace:^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.7.3",
    "eslint-plugin-unicorn": "^44.0.2"
  },
  "devDependencies": {
    "@types/confusing-browser-globals": "^1.0.0",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.11.5",
    "eslint": "^8.25.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-rajzik": "workspace:*"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "future.js",
    "lib/**/*.{js,map}",
    "node.js",
    "prettier.js",
    "react.js",
    "src/**/*.{ts,tsx,json}",
    "typescript.js"
  ],
  "group": "configs",
  "keywords": [
    "config",
    "eslint",
    "lumos"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "packemon": {
    "format": "lib",
    "platform": "node",
    "support": "current"
  },
  "peerDependencies": {
    "eslint": "^8.4.0",
    "prettier": "^2.5.1",
    "typescript": ">=4.5"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:rajzik/configs.git",
  "scripts": {
    "check-rules": "ts-node tools/check-rules/index.ts"
  },
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
