{
  "name": "eslint-config-rajzik",
  "version": "1.0.6",
  "description": "Reusable ESLint config for personal projects",
  "keywords": [
    "config",
    "eslint",
    "lumos"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "future.js",
    "lib/**/*.{js,map}",
    "node.js",
    "prettier.js",
    "react.js",
    "src/**/*.{ts,tsx,json}",
    "typescript.js"
  ],
  "scripts": {
    "build": "tsdown",
    "check-rules": "ts-node tools/check-rules/index.ts",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@rajzik/prettier-config",
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@rajzik/configs-shared": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.1",
    "eslint-plugin-unicorn": "^48.0.1"
  },
  "devDependencies": {
    "@rajzik/prettier-config": "workspace:*",
    "@types/confusing-browser-globals": "^1.0.2",
    "@types/eslint": "^8.44.6",
    "@types/node": "catalog:",
    "eslint": "^8.52.0",
    "prettier": "catalog:",
    "ts-node": "^10.9.1",
    "tsconfig-rajzik": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "eslint": "^8.4.0",
    "prettier": "^2.5.1 || ^3.0.0",
    "typescript": ">=4.5"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "configs"
}
