{
  "name": "jest-preset-rajzik",
  "description": "Jest preset for lumos configured projects",
  "version": "1.0.6",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "dependencies": {
    "@jest/types": "^29.6.1",
    "@rajzik/configs-shared": "workspace:^1.0.0",
    "@types/jest": "^29.5.2",
    "jest-junit": "^16.0.0"
  },
  "devDependencies": {
    "jest": "^29.6.1",
    "tsconfig-rajzik": "workspace:*"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "jest-preset.js",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "group": "configs",
  "keywords": [
    "jest-preset",
    "lumos"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "fileMock": "src/fileMock.ts"
    },
    "format": "lib",
    "platform": "node",
    "support": "current"
  },
  "peerDependencies": {
    "jest": "^28.0.0 || ^29.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:rajzik/configs.git",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
