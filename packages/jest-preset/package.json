{
  "name": "jest-preset-rajzik",
  "version": "1.0.6",
  "description": "Jest preset for lumos configured projects",
  "keywords": [
    "jest-preset",
    "lumos"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache dist dts .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "lint": "eslint --max-warnings 0 --cache --cache-location .cache/.eslintcache",
    "test": "jest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@rajzik/prettier-config",
  "dependencies": {
    "@jest/types": "^29.6.3",
    "@rajzik/configs-shared": "workspace:*",
    "@types/jest": "^29.5.14",
    "jest-junit": "^16.0.0"
  },
  "devDependencies": {
    "@rajzik/eslint-config": "workspace:*",
    "@rajzik/prettier-config": "workspace:*",
    "@rajzik/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "jest": "^29.7.0",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "configs"
}
