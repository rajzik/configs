{
  "name": "jest-preset-rajzik",
  "version": "1.0.6",
  "description": "Jest preset for lumos configured projects",
  "keywords": [
    "jest-preset",
    "lumos"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "jest-preset.js",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "test": "jest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@rajzik/prettier-config",
  "dependencies": {
    "@jest/types": "^29.6.3",
    "@rajzik/configs-shared": "workspace:*",
    "@types/jest": "^29.5.14",
    "jest-junit": "^16.0.0"
  },
  "devDependencies": {
    "@rajzik/prettier-config": "workspace:*",
    "jest": "^29.7.0",
    "prettier": "catalog:",
    "tsconfig-rajzik": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "configs"
}
