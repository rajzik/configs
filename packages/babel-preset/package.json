{
  "name": "@rajzik/babel-preset",
  "description": "Babel preset for personal projects.",
  "version": "1.0.2",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@babel/plugin-proposal-export-default-from": "^7.18.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.0",
    "@rajzik/configs-shared": "workspace:^1.0.0",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "babel-plugin-transform-dev": "^2.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-typescript-to-proptypes": "^2.0.0",
    "react-refresh": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "babel-test": "^0.2.4",
    "jest-file-snapshot": "^0.5.0",
    "tsconfig-rajzik": "workspace:*"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "group": "configs",
  "keywords": [
    "babel-preset",
    "rajzik"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "packemon": {
    "format": "lib",
    "platform": "node",
    "support": "current"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0",
    "core-js": "^3.21.0",
    "graphql-tag": "^2.12.6",
    "typescript": ">=4.5"
  },
  "peerDependenciesMeta": {
    "core-js": {
      "optional": true
    },
    "graphql-tag": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:rajzik/configs.git",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
