{
  "name": "@rajzik/danger-configuration",
  "description": "Reusable Danger.js scripts",
  "version": "1.0.6",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "dependencies": {
    "@rajzik/conventional-changelog-types": "workspace:^1.0.0",
    "danger": "^11.2.6"
  },
  "devDependencies": {
    "@types/node": "^18.16.18"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "group": "configs",
  "keywords": [
    "config",
    "danger",
    "lumos",
    "scripts"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:rajzik/configs.git",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
