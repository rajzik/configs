{
  "name": "@rajzik/danger-configuration",
  "version": "1.0.6",
  "description": "Reusable Danger.js scripts",
  "keywords": [
    "config",
    "danger",
    "lumos",
    "scripts"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache"
  },
  "prettier": "@rajzik/prettier-config",
  "dependencies": {
    "@rajzik/conventional-changelog-types": "workspace:*",
    "danger": "^13.0.4"
  },
  "devDependencies": {
    "@rajzik/prettier-config": "workspace:*",
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "tsconfig-rajzik": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "configs"
}
