{
  "name": "@rajzik/configs-shared",
  "version": "1.0.0",
  "description": "Shared functionality for personal configs.",
  "keywords": [
    "private"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@rajzik/prettier-config",
  "devDependencies": {
    "@rajzik/prettier-config": "workspace:*",
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "tsconfig-rajzik": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "configs"
}
