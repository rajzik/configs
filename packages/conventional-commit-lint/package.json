{
  "name": "@rajzik/conventional-commit-lint-config",
  "description": "Conventional commit lint config for personal projects",
  "version": "1.0.0",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "dependencies": {
    "@rajzik/conventional-changelog-types": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "files": [
    "dts",
    "dts/**/*.d.ts",
    "dts/**/*.{d.ts,d.ts.map}",
    "lib",
    "lib/**/*.{js,map}",
    "src",
    "src/**/*.{ts,tsx,json}"
  ],
  "group": "changelog",
  "keywords": [
    "commitlint",
    "conventional-changelog",
    "preset"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "packemon": {
    "format": "lib",
    "platform": "node",
    "support": "current"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=12"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:rajzik/configs.git",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
