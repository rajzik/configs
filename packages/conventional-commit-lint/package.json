{
  "name": "@rajzik/conventional-commit-lint-config",
  "version": "1.0.0",
  "description": "Conventional commit lint config for personal projects",
  "keywords": [
    "commitlint",
    "conventional-changelog",
    "preset"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dts/index.d.ts",
  "files": [
    "dts",
    "dts/**/*.d.ts",
    "dts/**/*.{d.ts,d.ts.map}",
    "lib",
    "lib/**/*.{js,map}",
    "src",
    "src/**/*.{ts,tsx,json}"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@rajzik/prettier-config",
  "dependencies": {
    "@rajzik/conventional-changelog-types": "workspace:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@rajzik/prettier-config": "workspace:*",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=12"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=8.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "changelog"
}
