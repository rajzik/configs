{
  "name": "conventional-changelog-rajzik",
  "description": "Conventional changelog preset for my personal projects",
  "version": "1.0.1",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "dependencies": {
    "@rajzik/conventional-changelog-types": "workspace:*"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.14",
    "@types/tmp": "^0.2.5",
    "conventional-changelog-core": "^7.0.0",
    "conventional-recommended-bump": "^9.0.0",
    "shelljs": "^0.8.5",
    "tmp": "^0.2.1"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "templates/**/*.hbs"
  ],
  "group": "changelog",
  "keywords": [
    "conventional-changelog",
    "preset",
    "rajzik"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "packemon": {
    "format": "lib",
    "platform": "node",
    "support": "current"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:rajzik/configs.git",
  "types": "./dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
