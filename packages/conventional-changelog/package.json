{
  "name": "conventional-changelog-rajzik",
  "version": "1.0.1",
  "description": "Conventional changelog preset for my personal projects",
  "keywords": [
    "conventional-changelog",
    "preset",
    "rajzik"
  ],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "templates/**/*.hbs"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@rajzik/conventional-changelog-types": "workspace:*"
  },
  "devDependencies": {
    "@rajzik/prettier-config": "workspace:*",
    "@types/shelljs": "^0.8.16",
    "@types/tmp": "^0.2.6",
    "conventional-changelog-core": "^9.0.0",
    "conventional-recommended-bump": "^11.1.0",
    "prettier": "catalog:",
    "shelljs": "^0.10.0",
    "tmp": "^0.2.3",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "changelog"
}
