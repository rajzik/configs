{
  "name": "@rajzik/prettier-config",
  "version": "1.0.0",
  "description": "Reusable Prettier config for personal projects",
  "keywords": [],
  "repository": "git@github.com:rajzik/configs.git",
  "license": "MIT",
  "author": "Jan Å ilhan <silhanu@gmail.com>",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache dist dts .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "./dist/index.js",
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-tailwindcss": "^0.6.12"
  },
  "devDependencies": {
    "@rajzik/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "group": "configs"
}
